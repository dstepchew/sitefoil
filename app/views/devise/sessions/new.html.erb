
 <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>


<div class="jumbotron">
  <div class="container">
    <span class="glyphicon glyphicon-hand-down"></span>
    <h2>Sign In</h2>
    <div class="box">
        <%= f.email_field :email, class: "", :autofocus => true, :placeholder => 'Email Address' %>
	    <%= f.password_field :password, :autofocus => true, :placeholder => 'password' %>
	    <%= button_tag( :class => "btn btn-default full-width") do %>
  		<span class="glyphicon glyphicon-ok"></span>
		<% end %>

	    
    </div>
  </div>
<div class="clearfix"></div>
</div>


 <% end %>
<div class="clearfix"></div>


<div class="panel-footer" style="display:none;">
    <%= render "devise/shared/links" %>
  </div>

<% content_for :body_class, "small-form" %>